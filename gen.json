{
    "containerDefinitions": [
        {
            "essential": true,
            "image": [
                "helloworld:latest"
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/bean-counter-service",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "name": "bean-counter-container",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080
                }
            ]
        }
    ],
    "cpu": "256",
    "executionRoleArn": [
        "arn:aws:iam::219104658389:role/ecsTaskExecutionRole"
    ],
    "family": "bean-counter-service-task",
    "memory": "1024",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "taskRoleArn": [
        "arn:aws:iam::219104658389:role/skarlekar-ecs-s3-full-access"
    ]
}
